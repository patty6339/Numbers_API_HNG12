{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.10"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/classify-number",
            "dest": "app.py"
        },
        {
            "src": "/",
            "dest": "app.py"
        },
        {
            "src": "/docs",
            "dest": "app.py"
        },
        {
            "src": "/redoc",
            "dest": "app.py"
        }
    ],
    "env": {
        "PYTHONUNBUFFERED": "1"
    }
}
